{
  "name": "ng2-rest-website",
  "version": "0.0.1",
  "description": "Simple REST app in Angular 2",
  "repository": "https://github.com/simpulton/angular2-rest-app",
  "scripts": {
    "clean": "rimraf node_modules doc typings && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "client": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3001",
    "server": "json-server --watch server/api/db.json",
    "lint": "tsconfig-lint",
    "start": "concurrent \"npm run server\" \"npm run client\"",
    "test": "karma start"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "lodash": "^4.12.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.7",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "exports-loader": "0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.12",
    "karma": "^0.13.22",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tsconfig-lint": "^0.11.0",
    "tslint": "^3.9.0",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "author": "Lukas Ruebbelke",
  "license": "MIT"
}
